      <style>
.mask-animation {
  clip-path: polygon(0% 0%, 0% 100%, 0% 100%, 0% 0%, 100% 0%, 100% 100%, 0% 100%, 0% 100%, 100% 100%, 100% 0%);
  animation-duration: 1.5s;
  animation-name: growSquare;
  animation-iteration-count: 1;
}
/* Default (Desktop) Animation */
@keyframes growSquare {
  0% {
    clip-path: polygon(
      0% 0%,         /* Top-left corner */
      0% 100%,       /* Bottom-left corner */
      50% 100%,      /* Midpoint of the bottom edge */
      50% 50%,       /* Top Left - Center */
      50% 50%,       /* Top Right - Center */
      50% 50%,       /* Bottom Right - Center */
      50% 50%,       /* Bottom Left - Center */
      50% 100%,      /* Midpoint of the bottom edge again */
      100% 100%,     /* Bottom-right corner */
      100% 0%        /* Top-right corner */
    );
  }
  95% {
    clip-path: polygon(
      0% 0%, 0% 100%, 0% 100%, 0% 0%, 
      100% 0%, 100% 100%, 
      0% 100%, 0% 100%, 
      100% 100%, 100% 0%
    );
    opacity: 1;
  }
  100% {
    clip-path: polygon(
      0% 0%, 0% 100%, 0% 100%, 0% 0%, 
      100% 0%, 100% 100%, 
      0% 100%, 0% 100%, 
      100% 100%, 100% 0%
    );
    opacity: 0;
  }
}
/* Mobile-Specific Animation */
@media (max-width: 768px) {
  @keyframes growSquare {
    0% {
      clip-path: polygon(
        0% 0%, 
        0% 100%, 
        45% 100%, 
        45% 50%, /* Top Left */
        55% 50%, /* Top Right */
        55% 50%, /* Bottom Right */
        45% 50%, /* Bottom Left */
        45% 100%, 
        100% 100%, 
        100% 0%
      );
    }
    95% {
      clip-path: polygon(
        0% 0%, 0% 100%, 0% 100%, 0% 0%, 
        100% 0%, 100% 100%, 
        0% 100%, 0% 100%, 
        100% 100%, 100% 0%
      );
      opacity: 1;
    }
    100% {
      clip-path: polygon(
        0% 0%, 0% 100%, 0% 100%, 0% 0%, 
        100% 0%, 100% 100%, 
        0% 100%, 0% 100%, 
        100% 100%, 100% 0%
      );
      opacity: 0;
    }
  }
}
