<div>
            <div class="container-large">
              <div class="products_heading_wrapper padding-global">
                <h2 class="products_heading">
{{ section.settings.text_collection_heading }}
</h2>
                <a href="{{ section.settings.url_button_link }}" class="button w-button"> {{ section.settings.text_button_text }} </a>
              </div>
              <div class="products_collection-list-wrapper w-dyn-list">
                <div id="w-node-f623e71b-c8b4-832b-c9e5-710503ff6bea-f55cc2be" role="list" class="products_collection-list w-dyn-items">
                  {% for product in section.settings.collection_collection.products limit: section.settings.collection_collection_limit %}
<div id="w-node-f623e71b-c8b4-832b-c9e5-710503ff6beb-f55cc2be" role="listitem" class="products_collection-item w-dyn-item">
    
                    
    <div id="w-node-f623e71b-c8b4-832b-c9e5-710503ff6bec-f55cc2be" class="collection-item_wrapper">
                      {% render 'product_item', product: product %}
                    </div>
    
                  
</div>
{% endfor %}
                </div>
                <div class="hide w-dyn-empty">
                  <div>No items found.</div>
                </div>
              </div>
              <div class="products_heading_wrapper padding-global is-slider-arrows">
                <div class="slider-arrows">
                  <a slider="left-arrow" href="#" class="slider-arrow w-inline-block"><svg width="100%" height="100%" viewbox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg" class="left-arrow">
                      <path fill-rule="evenodd" d="M11.354 1.646a.5.5 0 0 1 0 .708L5.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0z" fill="currentColor"></path>
                    </svg></a>
                  <a slider="right-arrow" href="#" class="slider-arrow w-inline-block"><svg width="100%" height="100%" viewbox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg" class="right-arrow">
                      <path fill-rule="evenodd" d="M4.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L10.293 8 4.646 2.354a.5.5 0 0 1 0-.708z" fill="currentColor"></path>
                    </svg></a>
                </div>
              </div>
              <div class="bottom-border"></div>
            </div>
          </div> {% schema %} {
    "tag": "section",
    "name": "products-1",
    "class": "section_products",
    "settings": [
        {
            "id": "text_collection_heading",
            "type": "text",
            "label": "Collection Heading",
            "default": "Coffee"
        },
        {
            "id": "url_button_link",
            "type": "url",
            "label": "Button Link"
        },
        {
            "id": "text_button_text",
            "type": "text",
            "label": "Button Text",
            "default": "Shop Coffee"
        },
        {
            "id": "collection_collection",
            "type": "collection",
            "label": "Collection"
        },
        {
            "type": "range",
            "id": "collection_collection_limit",
            "min": 1,
            "max": 99,
            "step": 1,
            "label": "Limit: Collection",
            "default": 6
        }
    ],
    "presets": [
        {
            "name": "products-1",
            "category": "Liquify",
            "blocks": []
        }
    ]
} {% endschema %}