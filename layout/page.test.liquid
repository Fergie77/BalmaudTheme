  <script>
  document.addEventListener("DOMContentLoaded", function () {
    const submitBtn = document.getElementById("submitYearButton");
    const birthYearInput = document.getElementById("birthYearInput");
    const mainContent = document.getElementById("mainContent");
    const ageGate = document.getElementById("ageGate");
    const errorMsg = document.getElementById("errorMsg");
    const minimumAge = 18;
    if (localStorage.getItem('ageVerified') === 'true') {
    if(ageGate){
    	ageGate.style.display = "none";
    }
      if(mainContent){
      mainContent.style.display = "block";
      }
    }
    else{
    ageGate.style.display = "flex";
    }
    submitBtn.addEventListener("click", function (e) {
      e.preventDefault();
      const birthYear = parseInt(birthYearInput.value);
      const currentYear = new Date().getFullYear();
      const age = currentYear - birthYear;
      if (!isNaN(age) && age >= minimumAge) {
        if (typeof localStorage !== "undefined") {
  localStorage.setItem("ageVerified", "true");
}
if(ageGate){
	ageGate.style.display = "none";
}
        if(mainContent){
        mainContent.style.display = "block";
        }
      } else {
        errorMsg.style.display = "block";
      }
    });
  });
</script>
  <script>
  (function () {
    const LOCALHOST_URL = [
      'http://localhost:5173/@vite/client',
      'http://localhost:5173/src/main.js',  // Updated path
    ]
    const PROD_URL = ['https://balmaud.netlify.app/main.js']  // Updated path
    function createScripts(arr, isDevMode) {
      return arr.map(function (url) {
        const s = document.createElement('script')
        s.src = url
        s.type = isDevMode ? 'module' : 'text/javascript'
        return s
      })
    }
    function insertScript(scriptArr) {
      scriptArr.forEach(function (script) {
        document.body.appendChild(script)
      })
    }
    const localhostScripts = createScripts(LOCALHOST_URL, true)
    const prodScripts = createScripts(PROD_URL, false)  // Changed to false for production
    let choosedScripts = null
    fetch(LOCALHOST_URL[0], {
      method: 'HEAD',
      mode: 'cors'
    })
      .then(() => {
        console.log('Development mode')
        choosedScripts = localhostScripts
      })
      .catch((e) => {
        console.log('Production mode')
        choosedScripts = prodScripts
      })
      .finally(() => {
        if (choosedScripts) {
          insertScript(choosedScripts)
        }
      })
  })()
</script>
